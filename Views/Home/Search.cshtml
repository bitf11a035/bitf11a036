@{
    ViewBag.Title = "Search";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model SoulMate.Models.User
<div class="latest_profile">
        <h5>Search Results</h5>
        <h5 class="create"><a href="/Home/Login">Create your profile now</a></h5>
        
      </div>

  <div class="profile_container">
@foreach(var data in ViewBag.data)
{
        <div class="profile_box">
          <div class="photo"><a href="http://www.free-css.com/"><img src="~/Images/pic_1.gif" alt="" /></a></div>
          <div class="profile_detail">
            <p>Name: <span>@data.Name</span><br />
              Height: <span>@data.Height</span><br />
              Bust: <span>@data.Country</span><br />
              Waist: <span>28</span><br />
              Hair: <span>Blackbrown</span></p>
          </div>
         </div>
}
